							"    }",
							"};",
							"pm.test(\"struktura json\", function () {",
							"    pm.response.to.have.jsonSchema(schema)",
							"});",
							"",
							"",
							"let req = request.data;",
							"let salary = parseInt(req.salary);",
							"",
							"pm.test(\"salary [0]\", function () {",
							"    pm.expect (pm.response.json().salary [0]).to.eql(salary)",
							"});",
							"    ",
							"pm.test(\"salary [1]\", function () { ",
							"    pm.expect (parseInt(pm.response.json().salary [1])).to.eql(salary * 2)",
							"});",
							"",
							"pm.test(\"salary [2]\", function () {",
							"    pm.expect (parseInt(pm.response.json().salary [2])).to.eql(salary *3)",
							"});",
							"",
							"",
							"pm.test (\"salary[2] > salary[1]\", function () {",
							"    pm.expect(parseInt(pm.response.json().salary[2])).to.greaterThan(parseInt(pm.response.json().salary[1]))",
							"});",
							"",
							"pm.test (\"salary[2] > salary[0]\", function () {",
							"    pm.expect (parseInt(pm.response.json().salary[2])).to.greaterThan(parseInt(pm.response.json().salary[0]))",
