		{
			"name": "2_user_info",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"",
							"let schema = {",
							"\"type\":\"object\",",
							"\"required\":[\"person\", \"qa_salary_after_12_months\", \"qa_salary_after_6_months\", \"start_qa_salary\"],",
							"\"properties\":{",
							"    \"person\":{",
							"        \"type\": \"object\",",
							"        \"required\":[\"u_age\", \"u_name\", \"u_salary_1_5_year\"],",
							"        \"properties\":{",
							"            \"u_age\": {\"type\": \"integer\"},",
							"            \"u_name\": {\"type\": \"array\"},",
							"            \"u_salary_1_5_year\": {\"type\": \"integer\"}",
							"        }",
							"    },",
							"    \"qa_salary_after_12_months\": {\"type\": \"integer\"},",
							"    \"qa_salary_after_6_months\": {\"type\": \"integer\"},",
							"    \"start_qa_salary\": {\"type\": \"integer\"}",
							"}",
							"}; ",
							"pm.test(\"struktura json\", function () {",
							"    pm.response.to.have.jsonSchema(schema);",
							"});",
							"",
							"",
							"pm.test(\"1_5_year\", function () {",
							"    pm.expect(pm.response.json().person.u_salary_1_5_year).to.eql(pm.environment.get (\"salary\")* 4);",
							"});",
							"",
							"pm.test(\"12_months\", function () {",
							"    let jsonData = pm.response.json();",
							"    pm.expect(jsonData.qa_salary_after_12_months).to.eql(pm.environment.get (\"salary\")* 2.9);",
							"});",
							"",
							"pm.test(\"6_months\", function () {",
							"    pm.expect(pm.response.json().qa_salary_after_6_months).to.eql(pm.environment.get (\"salary\")* 2);",
							"});",
							"",
							"",
							"let jsonData =JSON.parse(responseBody);",
							"postman.setEnvironmentVariable(\"u_salary\", jsonData.person.u_salary_1_5_year);"
						],
						"type": "text/javascript"
					}
				}
			],
